{
  "firebase": "swello2",
  "public": "build",
  "cleanUrls": true,
  "rewrites": [
    { "source": "/drawmap", "destination": "/index.html"},
    { "source": "/map/*", "destination": "/index.html"},
    { "source": "/favorite/*", "destination": "/index.html"},
    { "source": "/full/*", "destination": "/index.html"},
    { "source": "/error/*", "destination": "/index.html"},
  {
    "source": "**",
    "destination": "/index.html"
  } ],
  "ignore": [
    "firebase.json",
    "**/.*",
    "**/node_modules/**"
  ]
}